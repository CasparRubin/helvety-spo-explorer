{
  "name": "helvety-spo-explorer",
  "version": "1.0.1.5",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": ">=22.14.0 < 23.0.0"
  },
  "scripts": {
    "prebuild": "node scripts/inject-build-info.js",
    "prestart": "node scripts/inject-build-info.js",
    "build": "heft build --clean --production && heft package-solution --production",
    "start": "heft start --clean",
    "clean": "heft clean",
    "eject-webpack": "heft eject-webpack",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "predeploy": "npm run format:check && npm run type-check && npm run lint && npm run build"
  },
  "dependencies": {
    "@fluentui/react": "^8.125.0",
    "@microsoft/decorators": "1.22.1",
    "@microsoft/sp-application-base": "1.22.1",
    "@microsoft/sp-core-library": "1.22.1",
    "@microsoft/sp-dialog": "1.22.1",
    "@microsoft/sp-http": "1.22.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@microsoft/eslint-config-spfx": "1.22.1",
    "@microsoft/eslint-plugin-spfx": "1.22.1",
    "@microsoft/sp-module-interfaces": "1.22.1",
    "@microsoft/spfx-heft-plugins": "1.22.1",
    "@microsoft/spfx-web-build-rig": "1.22.1",
    "@rushstack/eslint-config": "4.5.2",
    "@rushstack/heft": "1.1.2",
    "@types/react": "17.0.62",
    "@types/react-dom": "17.0.20",
    "@types/webpack-env": "~1.15.2",
    "@typescript-eslint/parser": "8.46.2",
    "css-loader": "~7.1.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "typescript": "~5.8.0"
  },
  "overrides": {
    "@rushstack/heft": "1.1.2"
  }
}
